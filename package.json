{
  "name": "search-apps",
  "private": true,
  "version": "0.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "usecase:install": "cd usecase && npm install",
    "usecase:create": "schematics ./usecase:usecase --debug=false --force && npm run usecase:format",
    "usecase:build": "cd usecase && npm run build",
    "usecase:format": "prettier --write src/components/utils/renderApps.utils.tsx src/usecases/index.tsx src/utils/appName.utils.ts",
    "prepare": "husky install",
    "format": "prettier --write src/**/*.ts{,x}"
  },
  "dependencies": {
    "@angular-devkit/schematics-cli": "^14.2.2",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.1",
    "@mui/x-date-pickers": "^5.0.0",
    "browser-image-compression": "^2.0.0",
    "copy-image-clipboard": "^2.1.2",
    "dayjs": "^1.11.5",
    "dayjs-plugin-utc": "^0.1.2",
    "file-saver": "^2.0.5",
    "jspdf": "^2.5.1",
    "match-sorter": "^6.3.1",
    "mui-file-dropzone": "^4.0.2",
    "random-words": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-medium-image-zoom": "^5.1.2",
    "styled-components": "^5.3.6",
    "tesseract.js": "^3.0.3",
    "timezone-abbreviations": "^1.2.6",
    "timezones-list": "^3.0.1"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.5",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@types/styled-components-react-native": "^5.2.1",
    "@vitejs/plugin-react": "^2.1.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "typescript": "*",
    "vite": "^3.1.0"
  }
}
